<%= form_for(@artist) do |f| %>
  <div class="field">
  <%= f.label :name %>
  <%= f.text_field :name %>
  </div>
  <div class="field">
  <%= f.label :bio %>
  <%= f.text_area :bio, size: "60x12" %>
  </div>
  <div class="field">
  <%= f.file_field :artist_pic %>
  </div>
  <%= f.submit "Create" %>
<% end %>
